"UnLitGeneric"
{
	"$basetexture"		"sprites/physring1-"
	"$nocull" 1
	"$additive" 1
	"$vertexalpha" 1
	"$vertexcolor" 1
	"$ignorez"	0

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 20
		}
	}
}