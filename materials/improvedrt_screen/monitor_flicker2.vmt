"UnlitTwoTexture"
{
	$basetexture "matsys_regressiontest/background"
    !targettex1 "$basetexture"
    $vertexcolor 1
    $nolod 1

	"$texture2" "effects/flicker_256"
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$texture2"
			"animatedtextureframenumvar" "$frame2"
			"animatedtextureframerate" 24
		}
	}
}
